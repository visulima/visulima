{
  "openapi": "3.0.0",
  "info": {
    "description": "API Platform distribution for Next.js",
    "title": "nextjs_13_example_api-platform",
    "version": "0.0.0"
  },
  "paths": {
    "/api/cors": {
      "get": {
        "tags": [
          "root"
        ],
        "description": "Return a cors headers.",
        "operationId": "cors",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/error": {
      "get": {
        "tags": [
          "error"
        ],
        "description": "Returns a error message.",
        "operationId": "error",
        "parameters": [
          {
            "name": "errorType",
            "in": "query",
            "required": false,
            "explode": true,
            "schema": {
              "type": "string",
              "enum": [
                "server_error",
                "not_found_error"
              ]
            }
          }
        ],
        "responses": {
          "404": {
            "description": "Successful operation"
          },
          "500": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/api/hello": {
      "get": {
        "tags": [
          "root"
        ],
        "description": "Return a greeting.",
        "operationId": "hello",
        "parameters": [
          {
            "name": "name",
            "in": "query",
            "required": false,
            "explode": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {}
}