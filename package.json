{
    "name": "visulima",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "nx run-many --target=build --all --parallel",
        "build-storybook": "pnpm --filter \"storybook\" run build",
        "build:packages": "nx run-many --target=build --parallel --projects=connect,readdir,jsdoc-open-api,api-platform,crud,pagination,prisma-dmmf-transformer",
        "check": "manypkg check",
        "clean": "nx run-many --target=clean --all --parallel && rimraf node_modules",
        "docs:dev": "pnpm --filter \"docs\" run dev",
        "example:api-platform:dev": "pnpm --filter \"nextjs_example_api-platform\" run dev",
        "example:connect:dev": "pnpm --filter \"nextjs_example_connect\" run dev",
        "example:crud:dev": "pnpm --filter \"nextjs_example_crud\" run dev",
        "example:health-check:dev": "pnpm --filter \"nextjs_example_health_check\" run dev",
        "graph": "nx dep-graph",
        "preinstall": "node verify-node-version.cjs && pnpx only-allow pnpm",
        "lint:eslint": "nx run-many --target=lint:eslint --all --parallel",
        "lint:eslint:fix": "nx run-many --target=lint:eslint:fix --all --parallel",
        "lint:prettier": "prettier --config=.prettierrc.cjs --write '**/*.{json,yml,yaml,less,css,scss,sass}'",
        "lint:text": "textlint ./.github/ISSUE_TEMPLATE/** ./packages/** ./README.md ./UPGRADE.md --parallel --experimental --dry-run",
        "lint:text:fix": "textlint ./.github/ISSUE_TEMPLATE/** ./packages/** ./README.md ./UPGRADE.md --parallel --experimental --fix",
        "plop": "plop",
        "prettier": "prettier --config=.prettierrc.cjs --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
        "sort-package-json": "sort-package-json packages/**/package.json visulima/package.json ./package.json examples/**/package.json docs/package.json",
        "storybook": "pnpm --filter \"storybook\" run dev",
        "test:api-platform": "pnpm --filter \"api-platform\" run test",
        "test:connect": "pnpm --filter \"connect\" run test",
        "test:coverage": "nx run-many --target=coverage --all --parallel",
        "test:coverage:api-platform": "pnpm --filter \"api-platform\" run coverage",
        "test:coverage:connect": "pnpm --filter \"connect\" run coverage",
        "test:coverage:crud": "pnpm --filter \"crud\" run coverage",
        "test:coverage:jsdoc-open-api": "pnpm --filter \"jsdoc-open-api\" run coverage",
        "test:coverage:pagination": "pnpm --filter \"pagination\" run coverage",
        "test:coverage:prisma-dmmf-transformer": "pnpm --filter \"prisma-dmmf-transformer\" run coverage",
        "test:health-check": "pnpm --filter \"health-check\" run test",
        "test:jsdoc-open-api": "pnpm --filter \"jsdoc-open-api\" run test",
        "test:readdir": "pnpm --filter \"readdir\" run test",
        "visulima:dev": "pnpm --filter \"visulima\" run dev"
    },
    "browserslist": [
        "extends browserslist-config-anolilab"
    ],
    "dependencies": {
        "@anolilab/prettier-config": "^3.1.0",
        "@anolilab/textlint-config": "^4.1.0",
        "@manypkg/cli": "^0.19.2",
        "@nrwl/cli": "latest",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/tao": "^15.0.2",
        "@nrwl/workspace": "latest",
        "@octokit/rest": "^19.0.5",
        "@qiwi/multi-semantic-release": "6.5.1",
        "@types/node": "^18.11.5",
        "browserslist-config-anolilab": "^3.1.0",
        "commitizen": "^4.2.5",
        "core-js": "^3.26.0",
        "depcheck": "^1.4.3",
        "nx": "latest",
        "plop": "^3.1.1",
        "prettier": "^2.7.1",
        "prettier-plugin-prisma": "^4.4.0",
        "pretty-quick": "^3.1.3",
        "rimraf": "^3.0.2",
        "semantic-release": "^19.0.5",
        "sort-package-json": "^2.0.0",
        "textlint": "^12.2.2",
        "typescript": "^4.8.4"
    },
    "devDependencies": {
        "read-pkg": "^7.1.0",
        "tsup": "^6.3.0"
    },
    "packageManager": "pnpm@7.14.1",
    "engines": {
        "node": ">=16.0"
    },
    "prisma": {
        "schema": "db/schema.prisma"
    }
}
