{
  "name": "visulima",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all --parallel",
    "build-storybook": "pnpm --filter \"storybook\" run build",
    "build:docs": "pnpm --filter \"docs\" run build",
    "build:packages": "nx run-many --target=build --parallel --projects=connect,readdir,jsdoc-open-api,api-platform,crud,pagination,prisma-dmmf-transformer,health-check,nextra-theme-docs",
    "build:prod:nextra-theme-docs": "pnpm --filter \"nextra-theme-docs\" run build:prod",
    "build:prod:packages": "nx run-many --target=build:prod --parallel --projects=connect,readdir,jsdoc-open-api,api-platform,crud,pagination,prisma-dmmf-transformer,health-check,nextra-theme-docs",
    "build:prod:readdir": "pnpm --filter \"readdir\" run build:prod",
    "build:readdir": "pnpm --filter \"readdir\" run build",
    "check": "manypkg check",
    "clean": "nx run-many --target=clean --all --parallel && rimraf ./node_modules",
    "docs:build": "pnpm --filter \"docs\" run build",
    "docs:dev": "nx run-many --target=dev --parallel --projects=nextra-theme-docs,docs",
    "docs:copy:docs": "pnpm --filter \"docs\" run docs:copy",
    "example:api-platform:express:4:dev": "pnpm --filter \"express_example_api-platform\" run dev",
    "example:api-platform:next:12:dev": "pnpm --filter \"nextjs_12_example_api-platform\" run dev",
    "example:api-platform:next:13:dev": "pnpm --filter \"nextjs_13_example_api-platform\" run dev",
    "example:connect:dev": "pnpm --filter \"nextjs_example_connect\" run dev",
    "example:crud:next:12:dev": "pnpm --filter \"nextjs_12_example_crud\" run dev",
    "example:crud:next:13:dev": "pnpm --filter \"nextjs_13_example_crud\" run dev",
    "example:health-check:dev": "pnpm --filter \"nextjs_example_health_check\" run dev",
    "example:nextra:dev": "nx run-many --target=dev --parallel --projects=nextra-theme-docs,nextra_example",
    "graph": "nx dep-graph",
    "preinstall": "node verify-node-version.cjs && pnpx only-allow pnpm",
    "lint:eslint": "nx run-many --target=lint:eslint --all --parallel",
    "lint:eslint:api-platform": "pnpm --filter \"api-platform\" run lint:eslint",
    "lint:eslint:connect": "pnpm --filter \"connect\" run lint:eslint",
    "lint:eslint:crud": "pnpm --filter \"crud\" run lint:eslint",
    "lint:eslint:fix": "nx run-many --target=lint:eslint:fix --all --parallel",
    "lint:eslint:fix:api-platform": "pnpm --filter \"api-platform\" run lint:eslint:fix",
    "lint:eslint:fix:connect": "pnpm --filter \"connect\" run lint:eslint:fix",
    "lint:eslint:fix:crud": "pnpm --filter \"crud\" run lint:eslint:fix",
    "lint:eslint:fix:health-check": "pnpm --filter \"health-check\" run lint:eslint:fix",
    "lint:eslint:fix:jsdoc-open-api": "pnpm --filter \"jsdoc-open-api\" run lint:eslint:fix",
    "lint:eslint:fix:nextra-theme-docs": "pnpm --filter \"nextra-theme-docs\" run lint:eslint:fix",
    "lint:eslint:fix:pagination": "pnpm --filter \"pagination\" run lint:eslint:fix",
    "lint:eslint:fix:prisma-dmmf-transformer": "pnpm --filter \"prisma-dmmf-transformer\" run lint:eslint:fix",
    "lint:eslint:fix:readdir": "pnpm --filter \"readdir\" run lint:eslint:fix",
    "lint:eslint:health-check": "pnpm --filter \"health-check\" run lint:eslint",
    "lint:eslint:jsdoc-open-api": "pnpm --filter \"jsdoc-open-api\" run lint:eslint",
    "lint:eslint:nextra-theme-docs": "pnpm --filter \"nextra-theme-docs\" run lint:eslint",
    "lint:eslint:pagination": "pnpm --filter \"pagination\" run lint:eslint",
    "lint:eslint:prisma-dmmf-transformer": "pnpm --filter \"prisma-dmmf-transformer\" run lint:eslint",
    "lint:eslint:readdir": "pnpm --filter \"readdir\" run lint:eslint",
    "lint:prettier": "prettier --config=.prettierrc.cjs --write '**/*.{json,yml,yaml,less,css,scss,sass}'",
    "lint:text": "textlint ./.github/ISSUE_TEMPLATE/** ./packages/** ./README.md ./UPGRADE.md --parallel --experimental --dry-run",
    "lint:text:fix": "textlint ./.github/ISSUE_TEMPLATE/** ./packages/** ./README.md ./UPGRADE.md --parallel --experimental --fix",
    "plop": "plop",
    "prettier": "prettier --config=.prettierrc.cjs --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "sort-package-json": "sort-package-json packages/**/package.json visulima/package.json ./package.json examples/**/package.json docs/package.json",
    "storybook": "pnpm --filter \"storybook\" run dev",
    "test": "nx run-many --target=test --all --parallel",
    "test:api-platform": "pnpm --filter \"api-platform\" run test",
    "test:connect": "pnpm --filter \"connect\" run test",
    "test:coverage": "nx run-many --target=coverage --all --parallel",
    "test:coverage:api-platform": "pnpm --filter \"api-platform\" run coverage",
    "test:coverage:connect": "pnpm --filter \"connect\" run coverage",
    "test:coverage:crud": "pnpm --filter \"crud\" run coverage",
    "test:coverage:health-check": "pnpm --filter \"health-check\" run coverage",
    "test:coverage:jsdoc-open-api": "pnpm --filter \"jsdoc-open-api\" run coverage",
    "test:coverage:nextra-theme-docs": "pnpm --filter \"nextra-theme-docs\" run coverage",
    "test:coverage:pagination": "pnpm --filter \"pagination\" run coverage",
    "test:coverage:prisma-dmmf-transformer": "pnpm --filter \"prisma-dmmf-transformer\" run coverage",
    "test:coverage:readdir": "pnpm --filter \"readdir\" run coverage",
    "test:crud": "pnpm --filter \"crud\" run test",
    "test:health-check": "pnpm --filter \"health-check\" run test",
    "test:jsdoc-open-api": "pnpm --filter \"jsdoc-open-api\" run test",
    "test:nextra-theme-docs": "pnpm --filter \"nextra-theme-docs\" run test",
    "test:pagination": "pnpm --filter \"pagination\" run test",
    "test:prisma-dmmf-transformer": "pnpm --filter \"prisma-dmmf-transformer\" run test",
    "test:readdir": "pnpm --filter \"readdir\" run test",
    "visulima:dev": "pnpm --filter \"visulima\" run dev"
  },
  "browserslist": [
    "extends browserslist-config-anolilab"
  ],
  "dependencies": {
    "@anolilab/prettier-config": "^3.1.1",
    "@anolilab/textlint-config": "^4.2.2",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@manypkg/cli": "^0.20.0",
    "@nrwl/cli": "latest",
    "@nrwl/nx-cloud": "16.0.5",
    "@nrwl/tao": "16.2.2",
    "@nrwl/workspace": "16.2.2",
    "@octokit/rest": "^19.0.11",
    "@qiwi/multi-semantic-release": "6.7.0",
    "@tsconfig/node16": "^1.0.4",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^20.2.3",
    "browserslist-config-anolilab": "^3.2.1",
    "commitizen": "^4.3.0",
    "core-js": "^3.30.2",
    "depcheck": "^1.4.3",
    "husky": "^8.0.3",
    "nx": "latest",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "prettier-plugin-prisma": "^4.13.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.2",
    "sort-package-json": "^2.4.1",
    "textlint": "^13.3.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "load-tsconfig": "^0.2.5",
    "read-pkg": "^8.0.0",
    "tsup": "^6.7.0"
  },
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=16.18.0 <=20.*"
  },
  "pnpm": {
    "overrides": {
      "load-tsconfig": "^0.2.5",
      "vite": "4.3.4",
      "tsconfig-paths": "^4.0.0",
      "cosmiconfig-typescript-loader": "^4.3.0",
      "semantic-release": "^21.0.2"
    }
  }
}
