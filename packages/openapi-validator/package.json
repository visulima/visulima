{
    "name": "@visulima/openapi-validator",
    "version": "1.0.0",
    "description": "Fastest deep clone implementation.",
    "keywords": [
        "anolilab",
        "visulima",
        "openapi",
        "validator",
        "validation",
        "schema",
        "swagger",
        "swagger3",
        "swagger2",
        "swagger3.1"
    ],
    "type": "module",
    "homepage": "https://visulima.com/packages/openapi-validator",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/openapi-validator"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "module": "dist/index.js",
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.js",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:prettier": "prettier --config=.prettierrc.js --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.js --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:bench": "vitest bench",
        "test:watch": "vitest"
    },
    "dependencies": {
        "ajv": "^8.12.0",
        "ajv-errors": "^3.0.0",
        "ajv-draft-04": "^1.0.0",
        "ajv-formats": "^2.1.1"
    },
    "devDependencies": {
        "openapi-types": "^12.1.3",
        "type-fest": "^4.4.0",
        "@total-typescript/ts-reset": "^0.5.1",
        "@anolilab/eslint-config": "^14.0.24",
        "@anolilab/prettier-config": "^5.0.11",
        "@anolilab/semantic-release-preset": "^8.0.0",
        "@rushstack/eslint-plugin-security": "^0.7.1",
        "@types/node": "18.17.12",
        "@vitest/coverage-v8": "^0.34.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.51.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-mdx": "^2.2.0",
        "eslint-plugin-vitest": "^0.3.2",
        "eslint-plugin-vitest-globals": "^1.4.0",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "semantic-release": "^22.0.5",
        "sort-package-json": "^2.6.0",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2",
        "vitest": "^0.34.6"
    },
    "engines": {
        "node": ">=18.* <=20.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false
        }
    }
}
