{
    "name": "@visulima/nextra-theme-docs",
    "version": "1.0.3",
    "description": "A Visulima Nextra theme for documentation sites.",
    "keywords": [
        "anolilab",
        "visulima",
        "nextra",
        "theme",
        "docs"
    ],
    "homepage": "https://visulima.com/packages/nextra-theme-docs",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/nextra-theme-docs"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": [
        "./src/theme/polyfill.ts"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./style.css": "./style.css",
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "source": "src/index.tsx",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**",
        "style.css",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup && pnpm build:tailwind && pnpm minify:css",
        "build:prod": "cross-env NODE_ENV=production tsup && pnpm build:tailwind && pnpm minify:css",
        "build:tailwind": "NODE_ENV=production pnpm postcss css/styles.css -o style.css --verbose",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm predev && pnpm run build --watch",
        "lint:eslint": "cross-env NO_LOGS=true eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.cjs --cache --cache-strategy content .",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "minify:css": "lightningcss --minify --nesting --bundle --targets '>= 0.25%' style.css -o style.css",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@giscus/react": "^2.2.6",
        "@headlessui/react": "^1.7.7",
        "@heroicons/react": "^2.0.13",
        "@mdx-js/react": "^2.2.1",
        "@popperjs/core": "^2.11.6",
        "clsx": "^1.2.1",
        "copy-to-clipboard": "^3.3.3",
        "flexsearch": "^0.7.31",
        "focus-visible": "^5.2.0",
        "git-url-parse": "^13.1.0",
        "github-slugger": "^2.0.0",
        "intersection-observer": "^0.12.2",
        "match-sorter": "^6.3.1",
        "next-seo": "^5.15.0",
        "next-themes": "^0.2.1",
        "react-hot-toast": "^2.4.0",
        "scroll-into-view-if-needed": "^3.0.4"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^5.0.2",
        "@anolilab/semantic-release-preset": "^2.2.0",
        "@rushstack/eslint-plugin-security": "^0.5.0",
        "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
        "@tailwindcss/typography": "^0.5.9",
        "@types/flexsearch": "^0.7.3",
        "@types/git-url-parse": "^9.0.1",
        "@types/github-slugger": "^1.3.0",
        "@types/micromatch": "^4.0.2",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "@vitejs/plugin-react": "^3.0.1",
        "autoprefixer": "^10.4.13",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.32.0",
        "eslint-plugin-compat": "^4.0.2",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsonc": "^2.6.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-markdown": "^3.0.0",
        "eslint-plugin-material-ui": "^1.0.1",
        "eslint-plugin-no-loops": "^0.3.0",
        "eslint-plugin-no-secrets": "^0.8.9",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-optimize-regex": "^1.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-radar": "^0.2.1",
        "eslint-plugin-react": "7.32.1",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "^9.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-testing-library": "^5.10.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
        "eslint-plugin-you-dont-need-momentjs": "^1.6.0",
        "lightningcss-cli": "^1.18.0",
        "next": "^13.1.4",
        "nextra": "2.0.1",
        "postcss": "^8.4.21",
        "postcss-cli": "^10.1.0",
        "postcss-focus-visible": "^8.0.0",
        "postcss-import": "^15.1.0",
        "prettier": "^2.8.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "read-pkg": "^7.1.0",
        "rimraf": "^4.1.2",
        "semantic-release": "^20.1.0",
        "tailwindcss": "^3.2.4",
        "tsup": "^6.5.0",
        "typescript": "^4.9.4",
        "vitest": "^0.28.2"
    },
    "peerDependencies": {
        "next": ">=9.5.3",
        "nextra": ">=2.0.0",
        "react": ">=16.13.1",
        "react-dom": ">=16.13.1"
    },
    "engines": {
        "node": ">=16.18.0 <=19.*"
    },
    "publishConfig": {
        "access": "public"
    }
}
