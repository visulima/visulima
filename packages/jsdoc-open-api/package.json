{
    "name": "@visulima/jsdoc-open-api",
    "version": "1.1.1",
    "description": "Generates swagger doc based on JSDoc.",
    "keywords": [
        "visulima",
        "javascript",
        "openapi",
        "swagger",
        "jsdoc",
        "comment",
        "api",
        "webpack"
    ],
    "homepage": "https://visulima.com/packages/jsdoc-open-api",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/jsdoc-open-api"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "bin": {
        "jsdoc-open-api": "./bin/index.js"
    },
    "files": [
        "src",
        "dist",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist",
        "coverage": "vitest run --coverage",
        "dev": "pnpm predev && pnpm run build --watch",
        "lint:eslint": "cross-env NO_LOGS=true eslint --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.cjs",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "test": "vitest"
    },
    "dependencies": {
        "@apidevtools/swagger-parser": "^10.1.0",
        "@visulima/readdir": "1.2.0",
        "cli-progress": "^3.11.2",
        "commander": "^9.4.1",
        "comment-parser": "^1.3.1",
        "lodash.mergewith": "^4.6.2",
        "yaml": "^2.1.3"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^4.0.9",
        "@anolilab/semantic-release-preset": "^2.0.7",
        "@rushstack/eslint-plugin-security": "^0.5.0",
        "@types/commander": "^2.12.2",
        "@types/debug": "^4.1.7",
        "@types/http-errors": "^1.8.2",
        "@types/lodash.mergewith": "^4.6.7",
        "@types/node": "^18.8.4",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "c8": "^7.12.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.25.0",
        "eslint-plugin-compat": "^4.0.2",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsonc": "^2.5.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-markdown": "^3.0.0",
        "eslint-plugin-material-ui": "^1.0.1",
        "eslint-plugin-no-loops": "^0.3.0",
        "eslint-plugin-no-secrets": "^0.8.9",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-optimize-regex": "^1.2.1",
        "eslint-plugin-promise": "^6.0.1",
        "eslint-plugin-radar": "^0.2.1",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-testing-library": "^5.7.2",
        "eslint-plugin-unicorn": "^44.0.2",
        "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
        "eslint-plugin-you-dont-need-momentjs": "^1.6.0",
        "openapi-types": "^12.0.2",
        "prettier": "^2.7.1",
        "read-pkg": "^7.1.0",
        "rimraf": "^3.0.2",
        "semantic-release": "^19.0.5",
        "tsup": "^6.2.3",
        "typescript": "^4.8.4",
        "vitest": "^0.24.1",
        "webpack": "^5.74.0"
    },
    "optionalDependencies": {
        "webpack": "^5.74.0"
    },
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public"
    }
}
