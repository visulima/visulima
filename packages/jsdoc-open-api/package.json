{
    "name": "@visulima/jsdoc-open-api",
    "version": "1.3.10",
    "description": "Generates swagger doc based on JSDoc.",
    "keywords": [
        "visulima",
        "javascript",
        "openapi",
        "swagger",
        "jsdoc",
        "comment",
        "api",
        "webpack",
        "cli",
        "command"
    ],
    "homepage": "https://visulima.com/packages/jsdoc-open-api",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/jsdoc-open-api"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./cli": {
            "types": "./dist/cli/index.d.ts",
            "require": "./dist/cli/index.js",
            "import": "./dist/cli/index.mjs"
        },
        "./cli/commander": {
            "types": "./dist/cli/commander/index.d.ts",
            "require": "./dist/cli/commander/index.js",
            "import": "./dist/cli/commander/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "jsdoc-open-api": "./bin/index.js"
    },
    "files": [
        "bin/**",
        "cli/**",
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.js --cache --cache-strategy content .",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@apidevtools/swagger-parser": "^10.1.0",
        "@visulima/readdir": "1.3.6",
        "comment-parser": "^1.3.1",
        "lodash.mergewith": "^4.6.2",
        "read-pkg-up": "^7.0.1",
        "yaml": "^2.3.1"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^6.1.0",
        "@anolilab/prettier-config": "^4.0.6",
        "@anolilab/semantic-release-preset": "^3.1.1",
        "@rushstack/eslint-plugin-security": "^0.6.0",
        "@stoplight/types": "^13.15.0",
        "@types/cli-progress": "^3.11.0",
        "@types/http-errors": "^2.0.1",
        "@types/lodash.mergewith": "^4.6.7",
        "@types/node": "18.16.18",
        "@types/webpack": "^5.28.1",
        "@vitest/coverage-v8": "^0.32.2",
        "c8": "^8.0.0",
        "cli-progress": "^3.12.0",
        "commander": "^11.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.43.0",
        "openapi-types": "^12.1.3",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.1",
        "semantic-release": "^21.0.5",
        "tsup": "^7.0.0",
        "typescript": "^5.1.3",
        "vitest": "^0.32.2",
        "webpack": "^5.88.0"
    },
    "optionalDependencies": {
        "cli-progress": "^3.12.0",
        "commander": "^10.0.1",
        "webpack": "^5.86.0"
    },
    "engines": {
        "node": ">=16.18.0 <=20.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "sources": [
        "src/index.ts",
        "src/cli/index.ts",
        "src/cli/commander/index.ts"
    ]
}
