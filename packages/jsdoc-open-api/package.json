{
    "name": "@visulima/jsdoc-open-api",
    "version": "1.3.2",
    "description": "Generates swagger doc based on JSDoc.",
    "keywords": [
        "visulima",
        "javascript",
        "openapi",
        "swagger",
        "jsdoc",
        "comment",
        "api",
        "webpack",
        "cli",
        "command"
    ],
    "homepage": "https://visulima.com/packages/jsdoc-open-api",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/jsdoc-open-api"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./cli": {
            "types": "./dist/cli/index.d.ts",
            "require": "./dist/cli/index.js",
            "import": "./dist/cli/index.mjs"
        },
        "./cli/commander": {
            "types": "./dist/cli/commander/index.d.ts",
            "require": "./dist/cli/commander/index.js",
            "import": "./dist/cli/commander/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "jsdoc-open-api": "./bin/index.js"
    },
    "files": [
        "bin/**",
        "cli/**",
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm predev && pnpm run build --watch",
        "lint:eslint": "cross-env NO_LOGS=true eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.cjs --cache --cache-strategy content .",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@apidevtools/swagger-parser": "^10.1.0",
        "@visulima/readdir": "1.3.4",
        "comment-parser": "^1.3.1",
        "lodash.mergewith": "^4.6.2",
        "yaml": "^2.2.1"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^5.0.2",
        "@anolilab/semantic-release-preset": "^2.2.0",
        "@rushstack/eslint-plugin-security": "^0.5.0",
        "@types/cli-progress": "^3.11.0",
        "@types/http-errors": "^2.0.1",
        "@types/lodash.mergewith": "^4.6.7",
        "@types/node": "^18.11.18",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "c8": "^7.12.0",
        "cli-progress": "^3.11.2",
        "commander": "^10.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.32.0",
        "eslint-plugin-compat": "^4.0.2",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsonc": "^2.6.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-markdown": "^3.0.0",
        "eslint-plugin-material-ui": "^1.0.1",
        "eslint-plugin-no-loops": "^0.3.0",
        "eslint-plugin-no-secrets": "^0.8.9",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-optimize-regex": "^1.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-radar": "^0.2.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-testing-library": "^5.10.0",
        "eslint-plugin-unicorn": "^46.0.0",
        "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
        "eslint-plugin-you-dont-need-momentjs": "^1.6.0",
        "openapi-types": "^12.1.0",
        "prettier": "^2.8.3",
        "read-pkg": "^7.1.0",
        "rimraf": "^4.1.2",
        "semantic-release": "^21.0.0",
        "tsup": "^6.5.0",
        "typescript": "^4.9.4",
        "vitest": "^0.28.2",
        "webpack": "^5.75.0"
    },
    "optionalDependencies": {
        "cli-progress": "^3.11.2",
        "commander": "^10.0.0",
        "webpack": "^5.75.0"
    },
    "engines": {
        "node": ">=16.18.0 <=19.*"
    },
    "publishConfig": {
        "access": "public"
    },
    "sources": [
        "src/index.ts",
        "src/cli/index.ts",
        "src/cli/commander/index.ts"
    ]
}
