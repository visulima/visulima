{
    "name": "@visulima/jsdoc-open-api",
    "version": "2.0.85",
    "description": "Generates swagger doc based on JSDoc.",
    "keywords": [
        "visulima",
        "javascript",
        "openapi",
        "swagger",
        "jsdoc",
        "comment",
        "api",
        "webpack",
        "cli",
        "command"
    ],
    "homepage": "https://www.visulima.com/docs/package/jsdoc-open-api",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/jsdoc-open-api"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        },
        "./cli": {
            "require": {
                "types": "./dist/cli/index.d.ts",
                "default": "./dist/cli/index.js"
            },
            "import": {
                "types": "./dist/cli/index.d.mts",
                "default": "./dist/cli/index.mjs"
            }
        },
        "./cli/commander": {
            "require": {
                "types": "./dist/cli/commander/index.d.ts",
                "default": "./dist/cli/commander/index.js"
            },
            "import": {
                "types": "./dist/cli/commander/index.d.mts",
                "default": "./dist/cli/commander/index.mjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "jsdoc-open-api": "./bin/index.js"
    },
    "files": [
        "bin/**",
        "cli/**",
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --cache --concurrency auto ",
        "lint:eslint:fix": "eslint . --cache --concurrency auto  --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@apidevtools/swagger-parser": "^12.0.0",
        "@visulima/fs": "3.1.9",
        "comment-parser": "^1.4.1",
        "lodash.mergewith": "^4.6.2",
        "publint": "^0.3.14",
        "read-pkg-up": "^7.0.1",
        "yaml": "^2.8.1"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.3.3",
        "@anolilab/prettier-config": "^6.0.6",
        "@anolilab/semantic-release-pnpm": "^2.0.4",
        "@anolilab/semantic-release-preset": "^11.0.4",
        "@types/cli-progress": "^3.11.6",
        "@types/http-errors": "^2.0.5",
        "@types/lodash.mergewith": "^4.6.9",
        "@types/node": "24.7.2",
        "@types/webpack": "^5.28.5",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "cli-progress": "^3.12.0",
        "commander": "^14.0.1",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "cross-env": "^10.1.0",
        "eslint": "9.37.0",
        "openapi-types": "^12.1.3",
        "prettier": "^3.6.2",
        "rimraf": "6.0.1",
        "semantic-release": "24.2.9",
        "tsup": "^8.5.0",
        "typescript": "5.9.3",
        "vitest": "^3.2.4",
        "webpack": "^5.102.1"
    },
    "optionalDependencies": {
        "cli-progress": "^3.12.0",
        "commander": "^14.0.1",
        "webpack": "^5.102.1"
    },
    "engines": {
        "node": ">=18.0.0 <=23.x"
    },
    "os": [
        "darwin",
        "linux",
        "win32"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "sources": [
        "src/index.ts",
        "src/cli/index.ts",
        "src/cli/commander/index.ts"
    ]
}
