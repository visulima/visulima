        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "redoc": "^2.1.3",
        "rimraf": "^5.0.5",
        "secretlint": "8.0.0",
        "semantic-release": "^22.0.8",
        "sort-package-json": "^2.6.0",
        "styled-components": "^6.1.1",
        "swagger-ui-dist": "^4.19.1",
        "swagger-ui-react": "^4.19.1",
        "tsup": "^8.0.1",
        "type-fest": "^4.8.2",
        "typescript": "^5.3.2",
        "vitest": "^0.34.6",
        "webpack": "^5.89.0",
        "zod": "^3.22.4"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zod": "^3.22.4"
    },
    "optionalDependencies": {
        "@hapi/hapi": "^21.3.2",
        "@koa/router": "^12.0.1",
        "@visulima/crud": "2.0.9",
        "@visulima/readdir": "2.0.9",
        "chalk": "5.3.0",
        "commander": "^11.1.0",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "fastify": "^4.24.3",
        "koa": "^2.14.2",
        "next": "^13.5.6",
        "rate-limiter-flexible": "^3.0.4",
        "redoc": "^2.1.3",
        "swagger-ui-dist": "^4.19.1",
        "swagger-ui-react": "^4.19.1",
        "webpack": "^5.89.0"
    },
    "engines": {
        "node": ">=18.* <=21.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false,
                "testing-library-dom": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false,
            "info_on_testing_library_framework": false,
            "info_on_disabling_etc_no_deprecated": false
        }
    },
    "sources": [
        "src/index-browser.tsx",
        "src/index-server.ts",
        "src/framework/cli/index.ts",
        "src/framework/cli/commander/index.ts",
        "src/framework/next/index-browser.tsx",
        "src/framework/next/index-server.ts",
        "src/framework/next/routes/pages/swagger/index.tsx",
        "src/framework/next/routes/pages/redoc/index.tsx"
    ]
}
