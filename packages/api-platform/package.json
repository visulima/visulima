        "security",
        "brute",
        "force",
        "bruteforce",
        "brute-force",
        "attack",
        "accepts",
        "koa",
        "express",
        "hapi.js",
        "restify",
        "swagger",
        "openapi"
    ],
    "homepage": "https://www.visulima.com/docs/package/api-platform",
