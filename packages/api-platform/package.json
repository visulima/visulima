{
    "name": "@visulima/api-platform",
    "version": "1.2.12",
    "description": "Visulima API platform is a set of tools to build and consume web APIs",
    "keywords": [
        "anolilab",
        "visulima",
        "react",
        "blitz",
        "blitzjs",
        "rate",
        "limit",
        "ratelimit",
        "rate-limit",
        "middleware",
        "ip",
        "auth",
        "authorization",
        "security",
        "brute",
        "force",
        "bruteforce",
        "brute-force",
        "attack",
        "accepts",
        "koa",
        "express",
        "hapi.js",
        "restify",
        "swagger",
        "openapi"
    ],
    "homepage": "https://visulima.com/packages/api-platform",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/api-platform"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index-server.d.ts",
            "browser": "./dist/index-browser.js",
            "require": "./dist/index-server.js",
            "import": "./dist/index-server.mjs"
        },
        "./next": {
            "types": "./dist/framework/next/index-server.d.ts",
            "browser": "./dist/framework/next/index-browser.js",
            "require": "./dist/framework/next/index-server.js",
            "import": "./dist/framework/next/index-server.mjs"
        },
        "./next/pages/swagger": {
            "types": "./dist/framework/next/routes/pages/swagger/index.d.ts",
            "browser": "./dist/framework/next/routes/pages/swagger/index.js",
            "require": "./dist/framework/next/routes/pages/swagger/index.js",
            "import": "./dist/framework/next/routes/pages/swagger/index.js"
        },
        "./next/pages/redoc": {
            "types": "./dist/framework/next/routes/pages/redoc/index.d.ts",
            "browser": "./dist/framework/next/routes/pages/redoc/index.js",
            "require": "./dist/framework/next/routes/pages/redoc/index.js",
            "import": "./dist/framework/next/routes/pages/redoc/index.js"
        },
        "./cli": {
            "types": "./dist/framework/cli/index-server.d.ts",
            "require": "./dist/framework/cli/index-server.js",
            "import": "./dist/framework/cli/index-server.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index-server.js",
    "module": "dist/index-server.mjs",
    "browser": "dist/index-browser.mjs",
    "types": "dist/index-server.d.ts",
    "bin": {
        "api-platform": "./bin/index.js"
    },
    "files": [
        "bin/**",
        "next/**",
        "dist/**",
        "recipes/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "rimraf dist && cross-env NODE_ENV=development tsup",
        "build:prod": "rimraf dist && cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --ext js,cjs,mjs,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.js",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:prettier": "prettier --config=.prettierrc.js --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.js --write .",
        "lint:types": "tsc --noEmit --project tsconfig.json",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@visulima/connect": "1.3.6",
        "@visulima/jsdoc-open-api": "1.3.10",
        "accepts": "^1.3.8",
        "debug": "^4.3.4",
        "http-errors": "^2.0.0",
        "http-status-codes": "^2.2.0",
        "jstoxml": "^3.2.7",
        "lodash.merge": "^4.6.2",
        "schema-dts": "^1.1.2",
        "ts-japi": "^1.8.0",
        "yaml": "^2.3.1",
        "zod-to-ts": "^1.1.4"
    },
