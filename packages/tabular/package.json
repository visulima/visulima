{
    "name": "@visulima/tabular",
    "version": "1.0.5",
    "description": "Create beautiful ASCII tables and grids with customizable borders, padding, and alignment. Supports Unicode, colors, and ANSI escape codes.",
    "keywords": [
        "cli",
        "table",
        "grid",
        "tabular",
        "terminal",
        "console",
        "ascii",
        "unicode",
        "alignment",
        "ansi",
        "visulima"
    ],
    "homepage": "https://visulima.com/packages/tabular",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/visulima.git",
        "directory": "packages/tabular"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./style": {
            "import": {
                "types": "./dist/style.d.mts",
                "default": "./dist/style.mjs"
            },
            "require": {
                "types": "./dist/style.d.cts",
                "default": "./dist/style.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "typesVersions": {
        ">=5.0": {
            ".": [
                "./dist/index.d.ts"
            ],
            "style": [
                "./dist/style.d.ts"
            ]
        }
    },
    "files": [
        "CHANGELOG.md",
        "README.md",
        "dist"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development packem build",
        "build:prod": "cross-env NODE_ENV=production packem build",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:types": "tsc --noEmit",
        "lint:types:tabular": "pnpm --filter \"tabular\" run lint:types",
        "test": "vitest run",
        "test:bench": "vitest bench --run",
        "test:coverage": "vitest run --coverage",
        "test:coverage:tabular": "pnpm --filter \"tabular\" run test:coverage",
        "test:tabular": "pnpm --filter \"tabular\" run test",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:ui:tabular": "pnpm --filter \"tabular\" run test:ui",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.2.4",
        "@anolilab/semantic-release-pnpm": "1.1.11",
        "@anolilab/semantic-release-preset": "^10.0.5",
        "@arethetypeswrong/cli": "^0.18.1",
        "@babel/core": "^7.27.3",
        "@rushstack/eslint-plugin-security": "^0.10.0",
        "@secretlint/secretlint-rule-preset-recommend": "^9.3.3",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "22.15.27",
        "@visulima/colorize": "1.4.22",
        "@visulima/packem": "^1.19.4",
        "@visulima/string": "1.2.3",
        "@vitest/coverage-v8": "^3.1.4",
        "@vitest/ui": "^3.1.4",
        "ansi-regex": "^6.1.0",
        "cli-table3": "^0.6.5",
        "conventional-changelog-conventionalcommits": "9.0.0",
        "cross-env": "^7.0.3",
        "esbuild": "^0.25.5",
        "eslint": "9.27.0",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
        "eslint-plugin-mdx": "^3.4.2",
        "eslint-plugin-vitest": "^0.5.4",
        "eslint-plugin-vitest-globals": "^1.5.0",
        "jiti": "2.4.2",
        "rimraf": "^6.0.1",
        "secretlint": "9.3.3",
        "semantic-release": "^24.2.5",
        "table": "^6.9.0",
        "terminal-size": "4.0.0",
        "type-fest": "^4.41.0",
        "typedoc": "0.28.5",
        "typedoc-plugin-markdown": "4.6.3",
        "typedoc-plugin-rename-defaults": "0.7.3",
        "typescript": "^5.8.3",
        "vitest": "^3.1.4"
    },
    "engines": {
        "node": ">=18.0.0 <=24.x"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
