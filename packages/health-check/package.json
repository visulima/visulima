{
    "name": "@visulima/health-check",
    "version": "2.0.22",
    "description": "A library built to provide support for defining service health for node services. It allows you to register async health checks for your dependencies and the service itself, provides a health endpoint that exposes their status, and health metrics.",
    "keywords": [
        "anolilab",
        "visulima",
        "blitz",
        "blitzjs",
        "nextjs",
        "health",
        "check",
        "checks",
        "dns-check",
        "http-check",
        "ping-check"
    ],
    "homepage": "https://www.visulima.com/docs/package/health-check",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/health-check"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint . --cache --concurrency auto",
        "lint:eslint:fix": "eslint . --cache --concurrency auto --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "cacheable-lookup": "^7.0.0",
        "http-status-codes": "^2.3.0",
        "pingman": "^2.0.0"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.3.3",
        "@anolilab/prettier-config": "^6.0.6",
        "@anolilab/semantic-release-pnpm": "^2.0.4",
        "@anolilab/semantic-release-preset": "^11.0.4",
        "@arethetypeswrong/cli": "^0.18.2",
        "@babel/core": "^7.28.4",
        "@eslint-react/eslint-plugin": "2.2.0",
        "@rushstack/eslint-plugin-security": "^0.12.0",
        "@testing-library/react": "^16.3.0",
        "@types/node": "24.7.2",
        "@visulima/packem": "2.0.0-alpha.30",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "cross-fetch": "^4.1.0",
        "esbuild": "0.25.10",
        "eslint": "9.37.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.0",
        "eslint-plugin-react-perf": "3.3.3",
        "eslint-plugin-react-refresh": "^0.4.23",
        "eslint-plugin-react-you-might-not-need-an-effect": "0.5.6",
        "eslint-plugin-testing-library": "^7.13.1",
        "eslint-plugin-validate-jsx-nesting": "^0.1.1",
        "msw": "^2.11.5",
        "next": "^15.5.4",
        "node-mocks-http": "^1.17.2",
        "prettier": "^3.6.2",
        "publint": "^0.3.14",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "rimraf": "6.0.1",
        "semantic-release": "24.2.9",
        "typescript": "5.9.3",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=18.0.0 <=24.x"
    },
    "os": [
        "darwin",
        "linux",
        "win32"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
