{
    "name": "@visulima/health-check",
    "version": "2.0.0",
    "description": "A library built to provide support for defining service health for node services. It allows you to register async health checks for your dependencies and the service itself, provides a health endpoint that exposes their status, and health metrics.",
    "keywords": [
        "anolilab",
        "visulima",
        "blitz",
        "blitzjs",
        "nextjs",
        "health",
        "check",
        "checks",
        "dns-check",
        "http-check",
        "ping-check"
    ],
    "homepage": "https://visulima.com/packages/health-check",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/health-check"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.js --cache --cache-strategy content .",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:prettier": "prettier --config=.prettierrc.js --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.js --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "cacheable-lookup": "^7.0.0",
        "http-status-codes": "^2.2.0",
        "pingman": "^2.0.0"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^11.3.3",
        "@anolilab/prettier-config": "^5.0.3",
        "@anolilab/semantic-release-preset": "^6.0.4",
        "@rushstack/eslint-plugin-security": "^0.6.0",
        "@types/node": "18.17.1",
        "@vitest/coverage-v8": "^0.34.3",
        "cross-env": "^7.0.3",
        "cross-fetch": "^4.0.0",
        "eslint": "^8.48.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-mdx": "^2.2.0",
        "eslint-plugin-vitest": "^0.2.8",
        "eslint-plugin-vitest-globals": "^1.4.0",
        "next": "^13.4.12",
        "node-mocks-http": "^1.13.0",
        "prettier": "^3.0.2",
        "rimraf": "^5.0.1",
        "semantic-release": "^21.1.1",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2",
        "vitest": "^0.34.3"
    },
    "optionalDependencies": {
        "next": "^13.4.12"
    },
    "engines": {
        "node": ">=18.* <=20.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false
        }
    }
}
