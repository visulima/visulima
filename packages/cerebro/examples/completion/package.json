{
    "name": "cerebro-example-completion",
    "version": "1.0.0",
    "private": true,
    "description": "Cerebro CLI shell completion example",
    "type": "module",
    "scripts": {
        "build": "node cli.js build",
        "completion:bash": "node cli.js completion --shell=bash",
        "completion:fish": "node cli.js completion --shell=fish",
        "completion:powershell": "node cli.js completion --shell=powershell",
        "completion:zsh": "node cli.js completion --shell=zsh",
        "serve": "node cli.js serve",
        "start": "node cli.js help",
        "test": "node cli.js test"
    },
    "dependencies": {
        "@visulima/cerebro": "workspace:*"
    },
    "optionalDependencies": {
        "@bomb.sh/tab": "^0.0.7"
    }
}
