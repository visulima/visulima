{
    "name": "@visulima/bytes",
    "version": "0.0.0",
    "description": "Utility functions to make dealing with Uint8Arrays easier",
    "keywords": [
        "visulima",
        "bytes"
    ],
    "homepage": "https://visulima.com/packages/bytes",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/visulima.git",
        "directory": "packages/bytes"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "browser": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "typesVersions": {
        ">=5.0": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development packem build",
        "build:prod": "cross-env NODE_ENV=production packem build",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.2.10",
        "@anolilab/semantic-release-pnpm": "1.1.11",
        "@anolilab/semantic-release-preset": "^10.0.5",
        "@arethetypeswrong/cli": "^0.18.1",
        "@babel/core": "^7.27.4",
        "@secretlint/secretlint-rule-preset-recommend": "^9.3.3",
        "@std/bytes": "jsr:1.0.6",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "22.15.29",
        "@visulima/packem": "^1.19.4",
        "@vitest/coverage-v8": "^3.2.0",
        "@vitest/ui": "^3.2.0",
        "conventional-changelog-conventionalcommits": "9.0.0",
        "cross-env": "^7.0.3",
        "esbuild": "^0.25.5",
        "eslint": "9.28.0",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "secretlint": "9.3.3",
        "semantic-release": "^24.2.5",
        "typescript": "^5.8.3",
        "vitest": "^3.2.0"
    },
    "engines": {
        "node": ">=20.18 <=24.x"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
