{
    "name": "@visulima/inspector",
    "version": "1.0.20",
    "description": "Inspect utility for Node.js and Browsers.",
    "keywords": [
        "visulima",
        "inspector",
        "inspect",
        "util.inspect",
        "object",
        "stringify",
        "pretty",
        "dump",
        "vardump",
        "var-dump"
    ],
    "homepage": "https://visulima.com/packages/inspector",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/visulima.git",
        "directory": "packages/inspector"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "browser": "./dist/index.mjs",
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "browser": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "typesVersions": {
        ">=5.0": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development packem build",
        "build:prod": "cross-env NODE_ENV=production packem build",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs",
        "lint:eslint:fix": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:browser:chrome": "vitest run --browser.name=chromium --browser.headless=true --browser.provider=playwright",
        "test:browser:firefox": "vitest run --browser.name=firefox --browser.headless=true --browser.provider=playwright",
        "test:browser:webkit": "vitest run --browser.name=webkit --browser.headless=true --browser.provider=playwright",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest",
        "ts:coverage": "typescript-coverage-report"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^15.0.3",
        "@anolilab/prettier-config": "^5.0.14",
        "@anolilab/semantic-release-pnpm": "1.1.10",
        "@anolilab/semantic-release-preset": "^10.0.3",
        "@arethetypeswrong/cli": "^0.17.4",
        "@babel/core": "^7.26.9",
        "@rushstack/eslint-plugin-security": "^0.8.3",
        "@secretlint/secretlint-rule-preset-recommend": "^9.2.0",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "18.19.71",
        "@visulima/colorize": "1.4.20",
        "@visulima/packem": "1.18.5",
        "@vitest/browser": "^3.0.7",
        "@vitest/coverage-v8": "^3.0.7",
        "@vitest/ui": "^3.0.7",
        "cross-env": "^7.0.3",
        "esbuild": "0.25.0",
        "eslint": "8.57.0",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
        "eslint-plugin-mdx": "^3.1.5",
        "eslint-plugin-vitest": "0.4.1",
        "eslint-plugin-vitest-globals": "^1.5.0",
        "playwright": "^1.50.1",
        "prettier": "^3.5.2",
        "rimraf": "6.0.1",
        "secretlint": "9.2.0",
        "semantic-release": "^24.2.3",
        "type-fest": "^4.35.0",
        "typescript": "5.8.2",
        "typescript-coverage-report": "^1.1.1",
        "vitest": "^3.0.7",
        "webdriverio": "^9.10.1"
    },
    "engines": {
        "node": ">=18.0.0 <=23.x"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false,
                "etc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false,
            "info_on_disabling_etc_no_deprecated": false
        }
    }
}
