{
    "name": "@visulima/object",
    "version": "1.0.12",
    "description": "Helper functions for working with objects and arrays.",
    "keywords": [
        "visulima",
        "access",
        "del",
        "delete",
        "dot notation",
        "dot",
        "dotty",
        "get",
        "notation",
        "obj",
        "object",
        "path",
        "prop",
        "property",
        "set",
        "transform",
        "typescript"
    ],
    "homepage": "https://visulima.com/packages/object",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/visulima.git",
        "directory": "packages/object"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint . --cache --concurrency auto",
        "lint:eslint:fix": "eslint . --cache --concurrency auto --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "type-fest": "^5.0.1"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.3.3",
        "@anolilab/prettier-config": "^6.0.6",
        "@anolilab/semantic-release-pnpm": "2.0.4",
        "@anolilab/semantic-release-preset": "^11.0.4",
        "@arethetypeswrong/cli": "^0.18.2",
        "@secretlint/secretlint-rule-preset-recommend": "^11.2.5",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "24.7.2",
        "@visulima/packem": "2.0.0-alpha.30",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "deeks": "^3.1.1",
        "dot-prop": "^10.1.0",
        "esbuild": "0.25.10",
        "eslint": "9.37.0",
        "filter-obj": "^6.1.0",
        "is-plain-obj": "^4.1.0",
        "prettier": "^3.6.2",
        "publint": "^0.3.14",
        "rimraf": "6.0.1",
        "secretlint": "11.2.5",
        "semantic-release": "^24.2.9",
        "typescript": "5.9.3",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=20.19 <=24.x"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
