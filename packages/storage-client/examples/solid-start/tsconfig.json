{
    "compilerOptions": {
        "target": "ESNext",
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "paths": {
            "~/*": ["./src/*"]
        },
        "types": ["vinxi/types/client"],
        "allowJs": true,
        "strict": true,
        "noEmit": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true
    }
}
