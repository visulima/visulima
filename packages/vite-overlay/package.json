        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "24.3.0",
        "@visulima/packem": "2.0.0-alpha.18",
        "@visulima/path": "workspace:*",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "cross-env": "^10.0.0",
        "cssnano": "7.1.1",
        "esbuild": "^0.25.9",
        "eslint": "9.34.0",
        "lucide-static": "^0.542.0",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "0.6.14",
        "rimraf": "^6.0.1",
        "secretlint": "11.2.0",
        "semantic-release": "^24.2.7",
        "tailwindcss": "^4.1.12",
        "typescript": "^5.9.2",
        "vite": "^7.1.4",
        "vitest": "^3.2.4"
