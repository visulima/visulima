{
    "name": "@visulima/error",
    "version": "4.6.0",
    "description": "Error with more than just a message, stacktrace parsing.",
    "keywords": [
        "anolilab",
        "character",
        "code-frame",
        "codeframe",
        "column number",
        "column",
        "coordinate",
        "error",
        "exception",
        "index",
        "line number",
        "line",
        "location",
        "parser",
        "position",
        "stack",
        "stack-trace",
        "stacktrace",
        "string",
        "text",
        "trace",
        "v8",
        "source",
        "code",
        "frame",
        "callstack",
        "call",
        "source-code",
        "pretty-error",
        "pretty-exceptions",
        "pretty",
        "visulima"
    ],
    "homepage": "https://www.visulima.com/docs/package/error",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/visulima.git",
        "directory": "packages/error"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./code-frame": {
            "import": {
                "types": "./dist/code-frame/index.d.mts",
                "default": "./dist/code-frame/index.mjs"
            },
            "require": {
                "types": "./dist/code-frame/index.d.cts",
                "default": "./dist/code-frame/index.cjs"
            }
        },
        "./error": {
            "import": {
                "types": "./dist/error/index.d.mts",
                "default": "./dist/error/index.mjs"
            },
            "require": {
                "types": "./dist/error/index.d.cts",
                "default": "./dist/error/index.cjs"
            }
        },
        "./stacktrace": {
            "import": {
                "types": "./dist/stacktrace/index.d.mts",
                "default": "./dist/stacktrace/index.mjs"
            },
            "require": {
                "types": "./dist/stacktrace/index.d.cts",
                "default": "./dist/stacktrace/index.cjs"
            }
        },
        "./solution": {
            "import": {
                "types": "./dist/solution/index.d.mts",
                "default": "./dist/solution/index.mjs"
            },
            "require": {
                "types": "./dist/solution/index.d.cts",
                "default": "./dist/solution/index.cjs"
            }
        },
        "./solution/ai": {
            "import": {
                "types": "./dist/solution/ai/index.d.mts",
                "default": "./dist/solution/ai/index.mjs"
            },
            "require": {
                "types": "./dist/solution/ai/index.d.cts",
                "default": "./dist/solution/ai/index.cjs"
            }
        },
        "./solution/ai/prompt": {
            "import": {
                "types": "./dist/solution/ai/ai-prompt.d.mts",
                "default": "./dist/solution/ai/ai-prompt.mjs"
            },
            "require": {
                "types": "./dist/solution/ai/ai-prompt.d.cts",
                "default": "./dist/solution/ai/ai-prompt.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "typesVersions": {
        ">=5.0": {
            ".": [
                "./dist/index.d.ts"
            ],
            "code-frame": [
                "./dist/code-frame/index.d.ts"
            ],
            "error": [
                "./dist/error/index.d.ts"
            ],
            "stacktrace": [
                "./dist/stacktrace/index.d.ts"
            ],
            "solution": [
                "./dist/solution/index.d.ts"
            ],
            "solution/ai": [
                "./dist/solution/ai/index.d.ts"
            ],
            "solution/ai/prompt": [
                "./dist/solution/ai/ai-prompt.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint",
        "lint:eslint:fix": "eslint ./src --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit --project tsconfig.json",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.2.25",
        "@anolilab/prettier-config": "^6.0.4",
        "@anolilab/semantic-release-pnpm": "^2.0.3",
        "@anolilab/semantic-release-preset": "^11.0.3",
        "@arethetypeswrong/cli": "^0.18.2",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/command-line-args": "^5.2.3",
        "@types/node": "24.3.1",
        "@visulima/packem": "2.0.0-alpha.18",
        "@visulima/path": "1.4.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "ai": "^5.0.34",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "esbuild": "0.25.9",
        "eslint": "9.35.0",
        "is-plain-obj": "^4.1.0",
        "prettier": "^3.6.2",
        "rimraf": "6.0.1",
        "semantic-release": "24.2.7",
        "typescript": "5.9.2",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "ai": "^5.0.30"
    },
    "peerDependenciesMeta": {
        "ai": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=18.0.0 <=24.x"
    },
    "os": [
        "darwin",
        "linux",
        "win32"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false,
                "etc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false,
            "info_on_disabling_etc_no_deprecated": false
        }
    }
}
