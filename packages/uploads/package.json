            "require": "./dist/storage/gcs/index.js",
            "import": "./dist/storage/gcs/index.mjs"
        },
        "./next": {
            "types": "./dist/next/index.d.ts",
            "require": "./dist/next/index.js",
            "import": "./dist/next/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "aws/**",
        "dist/**",
        "gcs/**",
        "next/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist",
        "coverage": "vitest run --coverage && jest --runInBand --coverage",
        "dev": "pnpm predev && pnpm run build --watch",
        "lint:eslint": "cross-env NO_LOGS=true eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.cjs --cache --cache-strategy content .",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "test": "vitest",
        "test:jest": "jest --watch --maxWorkers=25%",
        "test:vitest": "vitest"
    },
