{
    "name": "@visulima/crud",
    "version": "1.0.12",
    "description": "visulima crud",
    "keywords": [
        "anolilab",
        "visulima",
        "api-platform",
        "api",
        "crud",
        "prisma"
    ],
    "homepage": "https://visulima.com/packages/crud",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/crud"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./next": {
            "types": "./dist/next/index.d.ts",
            "require": "./dist/next/index.js",
            "import": "./dist/next/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "next/**",
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "rimraf dist && cross-env NODE_ENV=development tsup",
        "build:prod": "rimraf dist && cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm run build --watch",
        "lint:eslint": "cross-env NO_LOGS=true eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.cjs --cache --cache-strategy content .",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@visulima/pagination": "2.0.4",
        "@visulima/prisma-dmmf-transformer": "1.0.7",
        "http-errors": "^2.0.0",
        "lodash.set": "^4.3.2",
        "path-to-regexp": "^6.2.1"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^5.0.8",
        "@anolilab/semantic-release-preset": "^3.1.0",
        "@prisma/client": "^4.15.0",
        "@rushstack/eslint-plugin-security": "^0.6.0",
        "@testing-library/react": "^14.0.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@types/http-errors": "^2.0.1",
        "@types/json-schema": "7.0.12",
        "@types/lodash.set": "^4.3.7",
        "@types/node": "18.16.17",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@typescript-eslint/parser": "^5.59.9",
        "@vitest/coverage-v8": "^0.32.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.42.0",
        "eslint-plugin-compat": "^4.1.4",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-markdown": "^3.0.0",
        "eslint-plugin-no-loops": "^0.3.0",
        "eslint-plugin-no-secrets": "^0.8.9",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-optimize-regex": "^1.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-testing-library": "^5.11.0",
        "eslint-plugin-unicorn": "^47.0.0",
        "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
        "eslint-plugin-you-dont-need-momentjs": "^1.6.0",
        "next": "^12.0.0 || ^13.0.0",
        "next-test-api-route-handler": "^4.0.0-canary.1",
        "node-mocks-http": "^1.12.2",
        "openapi-types": "^12.1.3",
        "prettier": "^2.8.8",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^5.0.1",
        "semantic-release": "^21.0.5",
        "tsup": "^6.7.0",
        "typescript": "^5.1.3",
        "vitest": "^0.32.0"
    },
    "engines": {
        "node": ">=16.18.0 <=20.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "optionalDependenciest": {
        "next": "^12.0.0 || ^13.0.0",
        "@prisma/client": "^3.0.0 || ^4.0.0"
    },
    "sources": [
        "src/index.ts",
        "src/next/index.ts"
    ]
}
