{
    "name": "@visulima/crud",
    "version": "2.0.41",
    "description": "visulima crud",
    "keywords": [
        "anolilab",
        "visulima",
        "api-platform",
        "api",
        "crud",
        "prisma"
    ],
    "homepage": "https://www.visulima.com/docs/package/crud",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/crud"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        },
        "./next": {
            "require": {
                "types": "./dist/next/index.d.ts",
                "default": "./dist/next/index.js"
            },
            "import": {
                "types": "./dist/next/index.d.mts",
                "default": "./dist/next/index.mjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --cache --concurrency auto ",
        "lint:eslint:fix": "eslint . --cache --concurrency auto  --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@visulima/pagination": "4.0.0",
        "@visulima/prisma-dmmf-transformer": "2.0.32",
        "http-errors": "^2.0.0",
        "path-to-regexp": "^8.3.0"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.4.2",
        "@anolilab/prettier-config": "^6.0.7",
        "@anolilab/semantic-release-pnpm": "^2.0.4",
        "@anolilab/semantic-release-preset": "^11.0.4",
        "@babel/core": "^7.28.4",
        "@eslint-react/eslint-plugin": "2.2.3",
        "@opentelemetry/api": "^1.9.0",
        "@prisma/client": "6.17.1",
        "@testing-library/react": "^16.3.0",
        "@types/http-errors": "^2.0.5",
        "@types/json-schema": "7.0.15",
        "@types/node": "24.9.1",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "cross-env": "^10.1.0",
        "dot-prop": "10.1.0",
        "eslint": "9.38.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.0",
        "eslint-plugin-react-perf": "3.3.3",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-react-you-might-not-need-an-effect": "0.6.1",
        "eslint-plugin-testing-library": "^7.13.3",
        "eslint-plugin-validate-jsx-nesting": "^0.1.1",
        "next": "^15.5.6",
        "next-test-api-route-handler": "^5.0.3",
        "node-mocks-http": "^1.17.2",
        "openapi-types": "^12.1.3",
        "prettier": "^3.6.2",
        "publint": "^0.3.15",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "rimraf": "6.0.1",
        "semantic-release": "25.0.1",
        "tsup": "^8.5.0",
        "typescript": "5.9.3",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=18.0.0 <=25.x"
    },
    "os": [
        "darwin",
        "linux",
        "win32"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "optionalDependenciest": {
        "next": ">=12.0 || >=13.0 || >=14.0 || >=15.0",
        "@prisma/client": "^3.0.0 || ^4.0.0"
    },
    "sources": [
        "src/index.ts",
        "src/next/index.ts"
    ]
}
