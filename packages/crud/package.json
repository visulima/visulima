{
    "name": "@visulima/crud",
    "version": "1.0.12",
    "description": "visulima crud",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "license": "MIT",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "homepage": "https://visulima.com/packages/crud",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/crud"
    },
    "keywords": [
        "anolilab",
        "visulima",
        "api-platform",
        "api",
        "crud",
        "prisma"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./next": {
            "types": "./dist/next/index.d.ts",
            "require": "./dist/next/index.js",
            "import": "./dist/next/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "next/**",
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "engines": {
        "node": ">=16.18.0 <=20.*"
    },
    "scripts": {
        "build": "rimraf dist && cross-env NODE_ENV=development tsup",
        "build:prod": "rimraf dist && cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --ext js,cjs,mjs,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.js",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:prettier": "prettier --config=.prettierrc.js --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.js --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@visulima/pagination": "2.0.4",
        "@visulima/prisma-dmmf-transformer": "1.0.7",
        "http-errors": "^2.0.0",
        "lodash.set": "^4.3.2",
        "path-to-regexp": "^6.2.1"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^10.0.3",
        "@anolilab/prettier-config": "^5.0.0",
        "@anolilab/semantic-release-preset": "^6.0.1",
        "@prisma/client": "4.16.2",
        "@rushstack/eslint-plugin-security": "^0.6.0",
        "@testing-library/react": "^14.0.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@types/http-errors": "^2.0.1",
        "@types/json-schema": "7.0.12",
        "@types/lodash.set": "^4.3.7",
        "@types/node": "18.16.18",
        "@vitest/coverage-v8": "^0.33.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.45.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-mdx": "^2.1.0",
        "eslint-plugin-vitest": "^0.2.6",
        "next": "^12.0.0 || ^13.0.0",
        "next-test-api-route-handler": "^4.0.0-canary.1",
        "node-mocks-http": "^1.12.2",
        "openapi-types": "^12.1.3",
        "prettier": "^3.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^5.0.1",
        "semantic-release": "^21.0.7",
        "tsup": "^7.1.0",
        "typescript": "^5.1.6",
        "vitest": "^0.33.0"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false,
                "react": false,
                "react-usememo": false,
                "react-hooks": false,
                "testing-library": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false
        }
    },
    "optionalDependenciest": {
        "next": "^12.0.0 || ^13.0.0",
        "@prisma/client": "^3.0.0 || ^4.0.0"
    },
    "sources": [
        "src/index.ts",
        "src/next/index.ts"
    ]
}
