{
    "name": "@visulima/pail",
    "version": "2.1.30",
    "description": "Highly configurable Logger for Node.js, Edge and Browser.",
    "keywords": [
        "ansi",
        "anolilab",
        "browser",
        "browser-logger",
        "callsite",
        "callsites",
        "cli",
        "color",
        "colorful",
        "colorize",
        "consola",
        "console",
        "console-logger",
        "debug",
        "error-logging",
        "file-logger",
        "file-logging",
        "filesystem-logger",
        "filter",
        "fs-logger",
        "handling exceptions",
        "json",
        "json-logger",
        "json-logging",
        "log level",
        "log",
        "log-cleaner",
        "log-rotation",
        "log4j",
        "log4js",
        "log4ts",
        "logger",
        "logging",
        "node",
        "stream",
        "pretty",
        "node-logger",
        "pail",
        "pino",
        "pretty-error",
        "pretty-log",
        "print",
        "progress",
        "redact",
        "rotating-log",
        "show error",
        "timer",
        "universal",
        "visulima",
        "warning-logging",
        "winston"
    ],
    "homepage": "https://www.visulima.com/docs/package/pail",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/visulima.git",
        "directory": "packages/pail"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de",
        "url": "https://danielbannert.com"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "browser": "./dist/index.browser.mjs",
            "import": {
                "types": "./dist/index.server.d.mts",
                "default": "./dist/index.server.mjs"
            },
            "require": {
                "types": "./dist/index.server.d.cts",
                "default": "./dist/index.server.cjs"
            }
        },
        "./browser": {
            "import": {
                "types": "./dist/index.browser.d.mts",
                "default": "./dist/index.browser.mjs"
            },
            "require": {
                "types": "./dist/index.browser.d.cts",
                "default": "./dist/index.browser.cjs"
            }
        },
        "./server": {
            "import": {
                "types": "./dist/index.server.d.mts",
                "default": "./dist/index.server.mjs"
            },
            "require": {
                "types": "./dist/index.server.d.cts",
                "default": "./dist/index.server.cjs"
            }
        },
        "./reporter": {
            "browser": "./dist/reporter.browser.mjs",
            "import": {
                "types": "./dist/reporter.server.d.mts",
                "default": "./dist/reporter.server.mjs"
            },
            "require": {
                "types": "./dist/reporter.server.d.cts",
                "default": "./dist/reporter.server.cjs"
            }
        },
        "./server/reporter": {
            "import": {
                "types": "./dist/reporter.server.d.mts",
                "default": "./dist/reporter.server.mjs"
            },
            "require": {
                "types": "./dist/reporter.server.d.cts",
                "default": "./dist/reporter.server.cjs"
            }
        },
        "./browser/reporter": {
            "browser": "./dist/reporter.browser.mjs",
            "import": {
                "types": "./dist/reporter.browser.d.mts",
                "default": "./dist/reporter.browser.mjs"
            },
            "require": {
                "types": "./dist/reporter.browser.d.cts",
                "default": "./dist/reporter.browser.cjs"
            }
        },
        "./processor": {
            "browser": "./dist/processor.browser.mjs",
            "import": {
                "types": "./dist/processor.server.d.mts",
                "default": "./dist/processor.server.mjs"
            },
            "require": {
                "types": "./dist/processor.server.d.cts",
                "default": "./dist/processor.server.cjs"
            }
        },
        "./server/processor": {
            "import": {
                "types": "./dist/processor.server.d.mts",
                "default": "./dist/processor.server.mjs"
            },
            "require": {
                "types": "./dist/processor.server.d.cts",
                "default": "./dist/processor.server.cjs"
            }
        },
        "./browser/processor": {
            "browser": "./dist/processor.browser.mjs",
            "import": {
                "types": "./dist/processor.browser.d.mts",
                "default": "./dist/processor.browser.mjs"
            },
            "require": {
                "types": "./dist/processor.browser.d.cts",
                "default": "./dist/processor.browser.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.server.cjs",
    "module": "dist/index.server.mjs",
    "browser": "dist/index.browser.mjs",
    "types": "dist/index.server.d.ts",
    "typesVersions": {
        ">=5.0": {
            ".": [
                "./dist/index.browser.d.ts",
                "./dist/index.server.d.ts"
            ],
            "browser": [
                "./dist/index.browser.d.ts"
            ],
            "server": [
                "./dist/index.server.d.ts"
            ],
            "reporter": [
                "./dist/reporter.browser.d.ts",
                "./dist/reporter.server.d.ts"
            ],
            "browser/reporter": [
                "./dist/reporter.browser.d.ts"
            ],
            "server/reporter": [
                "./dist/reporter.server.d.ts"
            ],
            "processor": [
                "./dist/processor.browser.d.ts",
                "./dist/processor.server.d.ts"
            ],
            "browser/processor": [
                "./dist/processor.browser.d.ts"
            ],
            "server/processor": [
                "./dist/processor.server.d.ts"
            ]
        }
    },
    "files": [
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint . --cache --concurrency auto",
        "lint:eslint:fix": "eslint . --cache --concurrency auto --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit || exit 0",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@visulima/colorize": "1.4.24",
        "@visulima/error": "4.6.2",
        "@visulima/fmt": "1.1.16",
        "@visulima/inspector": "1.0.25",
        "safe-stable-stringify": "^2.5.0",
        "type-fest": "^5.1.0"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.4.2",
        "@anolilab/prettier-config": "^6.0.7",
        "@anolilab/semantic-release-pnpm": "^2.0.4",
        "@anolilab/semantic-release-preset": "^11.0.4",
        "@arethetypeswrong/cli": "^0.18.2",
        "@babel/core": "^7.28.4",
        "@secretlint/secretlint-rule-preset-recommend": "^11.2.5",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "24.7.2",
        "@visulima/packem": "2.0.0-alpha.30",
        "@visulima/redact": "2.0.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "esbuild": "0.25.11",
        "eslint": "9.37.0",
        "prettier": "^3.6.2",
        "publint": "^0.3.14",
        "rimraf": "6.0.1",
        "rotating-file-stream": "^3.2.7",
        "secretlint": "11.2.5",
        "semantic-release": "24.2.9",
        "string-length": "^6.0.0",
        "terminal-size": "^4.0.0",
        "typescript": "5.9.3",
        "vitest": "^3.2.4",
        "wrap-ansi": "^9.0.2"
    },
    "peerDependencies": {
        "@visulima/redact": "2.0.0",
        "rotating-file-stream": "^3.1.1"
    },
    "peerDependenciesMeta": {
        "@visulima/redact": {
            "optional": true
        },
        "rotating-file-stream": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=18.0.0 <=24.x"
    },
    "os": [
        "darwin",
        "linux",
        "win32"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
