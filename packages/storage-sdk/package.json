{
    "name": "@visulima/upload-client",
    "version": "0.0.0",
    "description": "Client SDK for @visulima/upload - Upload files with resumable uploads and progress tracking",
    "keywords": [
        "anolilab",
        "visulima",
        "upload",
        "client",
        "sdk",
        "resumable",
        "tus",
        "multipart",
        "progress",
        "chunked-upload",
        "file-upload",
        "typescript",
        "browser",
        "nodejs"
    ],
    "homepage": "https://visulima.com/packages/uploads",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/upload-client"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs"
        }
    },
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=../upload/.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=../upload/.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.3.3",
        "@anolilab/prettier-config": "^6.0.6",
        "@anolilab/semantic-release-pnpm": "2.0.4",
        "@anolilab/semantic-release-preset": "^11.0.4",
        "@arethetypeswrong/cli": "^0.18.2",
        "@types/node": "^24.6.2",
        "@visulima/packem": "2.0.0-alpha.23",
        "eslint": "^9.37.0",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
