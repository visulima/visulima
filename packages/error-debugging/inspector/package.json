{
    "name": "@visulima/inspector",
    "version": "1.0.30",
    "description": "Inspect utility for Node.js and Browsers.",
    "keywords": [
        "visulima",
        "inspector",
        "inspect",
        "util.inspect",
        "object",
        "stringify",
        "pretty",
        "dump",
        "vardump",
        "var-dump"
    ],
    "homepage": "https://visulima.com/packages/inspector",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/visulima.git",
        "directory": "packages/error-debugging/inspector"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "browser": "./dist/index.mjs",
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "browser": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "typesVersions": {
        ">=5.0": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint . --cache --concurrency auto",
        "lint:eslint:fix": "eslint . --cache --concurrency auto --fix",
        "lint:package-json": "publint --strict",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:browser:chrome": "vitest run --browser.name=chromium --browser.headless=true --browser.provider=playwright",
        "test:browser:firefox": "vitest run --browser.name=firefox --browser.headless=true --browser.provider=playwright",
        "test:browser:webkit": "vitest run --browser.name=webkit --browser.headless=true --browser.provider=playwright",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest",
        "ts:coverage": "typescript-coverage-report"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.4.4",
        "@anolilab/semantic-release-pnpm": "3.1.0",
        "@anolilab/semantic-release-preset": "^12.0.1",
        "@arethetypeswrong/cli": "^0.18.2",
        "@secretlint/secretlint-rule-preset-recommend": "^11.2.5",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "24.10.1",
        "@visulima/colorize": "1.4.29",
        "@visulima/packem": "2.0.0-alpha.40",
        "@vitest/browser": "^4.0.15",
        "@vitest/coverage-v8": "^4.0.15",
        "@vitest/ui": "^4.0.15",
        "esbuild": "0.27.0",
        "eslint": "9.39.1",
        "eslint-plugin-playwright": "2.4.0",
        "playwright": "^1.57.0",
        "publint": "^0.3.15",
        "rimraf": "6.1.2",
        "secretlint": "11.2.5",
        "semantic-release": "^25.0.2",
        "type-fest": "^5.3.0",
        "typescript": "5.9.3",
        "typescript-coverage-report": "^1.1.1",
        "vitest": "^4.0.15",
        "webdriverio": "^9.21.0"
    },
    "engines": {
        "node": ">=22.12 <=25.x"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
