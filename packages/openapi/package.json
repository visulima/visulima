{
    "name": "@visulima/openapi",
    "version": "1.0.0",
    "description": "OpenApi generator and validator",
    "keywords": [
        "visulima",
        "javascript",
        "openapi",
        "swagger",
        "jsdoc",
        "comment",
        "api",
        "webpack",
        "cli",
        "command",
        "unplugin",
        "vite",
        "rollup",
        "esbuild",
        "nuxt",
        "rspack"
    ],
    "homepage": "https://visulima.com/packages/openapi",
    "bugs": {
        "url": "https://github.com/visulima/visulima/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/openapi"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        },
        "./cli": {
            "require": {
                "types": "./dist/cli/index.d.ts",
                "default": "./dist/cli/index.js"
            },
            "import": {
                "types": "./dist/cli/index.d.mts",
                "default": "./dist/cli/index.mjs"
            }
        },
        "./cli/commander": {
            "require": {
                "types": "./dist/cli/commander/index.d.ts",
                "default": "./dist/cli/commander/index.js"
            },
            "import": {
                "types": "./dist/cli/commander/index.d.mts",
                "default": "./dist/cli/commander/index.mjs"
            }
        },
        "./esbuild": {
            "require": {
                "types": "./dist/esbuild.d.ts",
                "default": "./dist/esbuild.js"
            },
            "import": {
                "types": "./dist/esbuild.d.mts",
                "default": "./dist/esbuild.mjs"
            }
        },
        "./nuxt": {
            "require": {
                "types": "./dist/nuxt.d.ts",
                "default": "./dist/nuxt.js"
            },
            "import": {
                "types": "./dist/nuxt.d.mts",
                "default": "./dist/nuxt.mjs"
            }
        },
        "./rollup": {
            "require": {
                "types": "./dist/rollup.d.ts",
                "default": "./dist/rollup.js"
            },
            "import": {
                "types": "./dist/rollup.d.mts",
                "default": "./dist/rollup.mjs"
            }
        },
        "./vite": {
            "require": {
                "types": "./dist/vite.d.ts",
                "default": "./dist/vite.js"
            },
            "import": {
                "types": "./dist/vite.d.mts",
                "default": "./dist/vite.mjs"
            }
        },
        "./webpack": {
            "require": {
                "types": "./dist/webpack.d.ts",
                "default": "./dist/webpack.js"
            },
            "import": {
                "types": "./dist/webpack.d.mts",
                "default": "./dist/webpack.mjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "openapi-generator": "./bin/openapi-generator.js"
    },
    "files": [
        "bin/**",
        "cli/**",
        "dist/**",
        "schemas/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.js",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:prettier": "prettier --config=.prettierrc.js --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.js --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@visulima/openapi-comment-parser": "1.0.0",
        "@visulima/readdir": "2.0.2",
        "@visulima/deep-clone": "1.0.0",
        "ajv": "^8.12.0",
        "ajv-draft-04": "^1.0.0",
        "ajv-formats": "^2.1.1",
        "chalk": "5.3.0",
        "cli-progress": "^3.12.0",
        "fast-safe-stringify": "^2.1.1",
        "lodash.merge": "^4.6.2",
        "lodash.mergewith": "^4.6.2",
        "unplugin": "^1.4.0",
        "json-pointer": "^0.6.1",
        "bl": "^6.0.7",
        "yaml": "^2.3.2"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^11.3.5",
        "@anolilab/prettier-config": "^5.0.4",
        "@anolilab/semantic-release-preset": "^6.0.5",
        "@nuxt/kit": "^3.6.5",
        "@nuxt/schema": "^3.6.5",
        "@types/json-pointer": "^1.0.32",
        "@total-typescript/ts-reset": "^0.5.1",
        "@octokit/rest": "^20.0.1",
        "@rspack/core": "^0.3.2",
        "@rushstack/eslint-plugin-security": "^0.6.0",
        "@types/cli-progress": "^3.11.1",
        "@types/http-errors": "^2.0.1",
        "@types/lodash.merge": "^4.6.7",
        "@types/lodash.mergewith": "^4.6.7",
        "@types/node": "18.17.12",
        "@types/webpack-sources": "^3.2.0",
        "@vitest/coverage-v8": "^0.34.3",
        "commander": "^11.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.48.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-mdx": "^2.2.0",
        "eslint-plugin-vitest": "^0.3.1",
        "eslint-plugin-vitest-globals": "^1.4.0",
        "giget": "^1.1.2",
        "openapi-types": "^12.1.3",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.1",
        "rollup": "^3.29.0",
        "semantic-release": "^21.1.1",
        "sort-package-json": "^2.5.1",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
        "vitest": "^0.34.3",
        "webpack": "^5.88.2",
        "webpack-sources": "^3.2.3"
    },
    "peerDependencies": {
        "@nuxt/kit": "^3",
        "@nuxt/schema": "^3",
        "commander": "^11.0.0",
        "esbuild": "*",
        "rollup": "^3",
        "vite": ">=3",
        "webpack": "^4 || ^5",
        "webpack-sources": "^3.2.3"
    },
    "peerDependenciesMeta": {
        "@nuxt/kit": {
            "optional": true
        },
        "@nuxt/schema": {
            "optional": true
        },
        "commander": {
            "optional": true
        },
        "esbuild": {
            "optional": true
        },
        "rollup": {
            "optional": true
        },
        "vite": {
            "optional": true
        },
        "webpack": {
            "optional": true
        },
        "webpack-sources": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=18.* <=20.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false
        }
    },
    "sources": [
        "src/index.ts",
        "src/cli/index.ts",
        "src/cli/commander/index.ts",
        "src/esbuild.ts",
        "src/nuxt.ts",
        "src/rollup.ts",
        "src/vite.ts",
        "src/webpack.ts"
    ]
}
