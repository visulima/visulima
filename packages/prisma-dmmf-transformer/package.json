{
    "name": "@visulima/prisma-dmmf-transformer",
    "version": "1.0.7",
    "description": "A generator for Prisma to generate a valid JSON Schema v7.",
    "keywords": [
        "anolilab",
        "visulima",
        "prisma",
        "prisma2",
        "prisma3",
        "prisma4",
        "prisma-schema",
        "dmmf",
        "transformer",
        "json-schema"
    ],
    "homepage": "https://visulima.com/packages/prisma-dmmf-transformer",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/prisma-dmmf-transformer"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist .eslintcache",
        "coverage": "vitest run --coverage",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.js --cache --cache-strategy content .",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:prettier": "prettier --config=.prettierrc.js --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.js --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@prisma/generator-helper": "5.0.0",
        "@prisma/internals": "5.0.0"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^10.0.5",
        "@anolilab/prettier-config": "^5.0.1",
        "@anolilab/semantic-release-preset": "^6.0.2",
        "@prisma/client": "4.16.2",
        "@rushstack/eslint-plugin-security": "^0.6.0",
        "@types/json-schema": "7.0.12",
        "@types/micromatch": "4.0.2",
        "@types/node": "18.16.18",
        "@vitest/coverage-v8": "0.33.0",
        "ajv": "8.12.0",
        "ajv-formats": "2.1.1",
        "cross-env": "7.0.3",
        "eslint": "^8.45.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-mdx": "^2.1.0",
        "eslint-plugin-vitest": "^0.2.6",
        "prettier": "3.0.0",
        "prisma": "4.16.2",
        "rimraf": "5.0.1",
        "semantic-release": "21.0.7",
        "tsup": "7.1.0",
        "typescript": "5.1.6",
        "vitest": "0.33.0"
    },
    "peerDependencies": {
        "@prisma/client": "3.* || 4.*",
        "prisma": "3.* || 4.*"
    },
    "engines": {
        "node": ">=16.18.0 <=20.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false
        }
    }
}
