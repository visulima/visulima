{
  "name": "@visulima/vite-plugin-next-error-overlay",
  "version": "0.1.0",
  "description": "Vite error overlay styled like Next.js 15.2 dev overlay.",
  "keywords": ["vite", "plugin", "error", "overlay", "nextjs"],
  "homepage": "https://visulima.com/",
  "bugs": { "url": "https://github.com/visulima/visulima/issues" },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visulima/visulima.git",
    "directory": "packages/vite-plugin-next-error-overlay"
  },
  "license": "MIT",
  "author": { "name": "Visulima" },
  "sideEffects": false,
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup",
    "build:prod": "cross-env NODE_ENV=production tsup",
    "clean": "rimraf node_modules dist .eslintcache",
    "dev": "tsup --watch",
    "lint:eslint": "eslint ./",
    "lint:eslint:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "test": "vitest run --typecheck"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "devDependencies": {
    "typescript": "5.8.3"
  },
  "engines": { "node": ">=18.0.0 <=23.x" }
}