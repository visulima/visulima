        "ts-node": "^10.9.2",
        "typescript": "^5.6.2",
        "vitest": "^2.1.1"
    },
    "optionalDependencies": {
        "@aws-sdk/client-s3": "^3.651.0",
        "@aws-sdk/credential-providers": "^3.650.0",
