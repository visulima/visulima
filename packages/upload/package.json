{
    "name": "@visulima/upload",
    "version": "0.0.0",
    "description": "Visulima upload - Store files in a web-accessible location via a simplified API. Can automatically scale and rotate images. Includes S3, Azure, GCS and local filesystem-based backends with the most convenient features of each.",
    "keywords": [
        "anolilab",
        "visulima",
        "blitz",
        "blitzjs",
        "next",
        "nextjs",
        "express",
        "upload",
        "uploads",
        "file",
        "files",
        "chunk",
        "resumable",
        "chunked-uploads",
        "tus",
        "form",
        "post",
        "multipart",
        "form-data",
        "formdata",
        "middleware",
        "s3",
        "azure",
        "aws",
        "gcs",
        "google",
        "storage"
    ],
    "homepage": "https://visulima.com/packages/uploads",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/uploads"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./azure": {
            "types": "./dist/storage/azure/index.d.ts",
            "require": "./dist/storage/azure/index.js",
            "import": "./dist/storage/azure/index.mjs"
        },
        "./aws": {
            "types": "./dist/storage/aws/index.d.ts",
            "require": "./dist/storage/aws/index.js",
            "import": "./dist/storage/aws/index.mjs"
        },
        "./gcs": {
            "types": "./dist/storage/gcs/index.d.ts",
            "require": "./dist/storage/gcs/index.js",
            "import": "./dist/storage/gcs/index.mjs"
        },
        "./next": {
            "types": "./dist/next/index.d.ts",
            "require": "./dist/next/index.js",
            "import": "./dist/next/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "aws/**",
        "dist/**",
        "gcs/**",
        "next/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development packem build",
        "build:prod": "cross-env NODE_ENV=production packem build",
        "clean": "rimraf node_modules dist",
        "coverage": "vitest run --coverage && jest --runInBand --coverage",
        "dev": "pnpm predev && pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "cross-env NO_LOGS=true eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.cjs --cache --cache-strategy content .",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "test": "vitest",
        "test:jest": "jest --watch --maxWorkers=25%",
        "test:vitest": "vitest"
    },
    "dependencies": {
        "@mjackson/multipart-parser": "^0.6.3",
        "@sindresorhus/fnv1a": "3.1.0",
        "@visulima/humanizer": "workspace:*",
        "@visulima/pagination": "workspace:*",
        "@visulima/path": "workspace:*",
        "bytes": "^3.1.2",
        "etag": "^1.8.1",
