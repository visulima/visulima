        "@types/http-errors": "^2.0.5",
        "@types/jest": "^30.0.0",
        "@types/lodash.merge": "^4.6.9",
        "@types/mime": "^4.0.0",
        "@types/multiparty": "4.2.1",
        "@types/node": "^24.5.2",
