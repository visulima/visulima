{
    "name": "@visulima/upload",
    "version": "0.0.0",
    "description": "Visulima upload - Store files in a web-accessible location via a simplified API. Can automatically scale and rotate images. Includes S3, Azure, GCS and local filesystem-based backends with the most convenient features of each.",
    "keywords": [
        "anolilab",
        "visulima",
        "blitz",
        "blitzjs",
        "next",
        "nextjs",
        "express",
        "upload",
        "uploads",
        "file",
        "files",
        "chunk",
        "resumable",
        "chunked-uploads",
        "tus",
        "form",
        "post",
        "multipart",
        "form-data",
        "formdata",
        "middleware",
        "s3",
        "azure",
        "aws",
        "gcs",
        "google",
        "storage"
    ],
    "homepage": "https://visulima.com/packages/uploads",
    "repository": {
        "type": "git",
        "url": "https://github.com/visulima/visulima.git",
        "directory": "packages/uploads"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./azure": {
            "types": "./dist/storage/azure/index.d.ts",
            "import": "./dist/storage/azure/index.js"
        },
        "./aws": {
            "types": "./dist/storage/aws/index.d.ts",
            "import": "./dist/storage/aws/index.js"
        },
        "./gcs": {
            "types": "./dist/storage/gcs/index.d.ts",
            "import": "./dist/storage/gcs/index.js"
        },
        "./next": {
            "types": "./dist/next/index.d.ts",
            "import": "./dist/next/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "aws/**",
        "dist/**",
        "gcs/**",
        "next/**",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:debug": "playwright test --debug",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:ui": "playwright test --ui",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "overrides": {
        "@aws-sdk/types": "^3.893.0",
        "node-fetch": "^4.0.0-beta.4"
    },
    "dependencies": {
        "@mjackson/multipart-parser": "^0.10.1",
