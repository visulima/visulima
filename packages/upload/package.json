        "@aws-sdk/signature-v4-crt": "^3.896.0",
        "@azure/storage-blob": "^12.28.0",
        "aws-crt": "^1.27.3",
        "express": "^5.1.0",
        "gaxios": "^7.1.2",
        "google-auth-library": "^10.3.0",
        "next": "^12.0.0 || ^13.0.0",
        "node-fetch": "^3.3.2"
    },
    "engines": {
        "node": ">=16.18.0 <=19.0"
