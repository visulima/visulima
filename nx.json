{
    "targetDefaults": {
        "build": {
            "cache": true
        },
        "build:prod": {
            "cache": true
        },
        "lint:eslint": {
            "dependsOn": ["^build", "^lint:prettier"],
            "inputs": ["default", "{workspaceRoot}/.eslintrc.cjs", "{workspaceRoot}/.eslintrc.js"],
            "cache": true
        },
        "lint:eslint:fix": {
            "dependsOn": ["^build", "^lint:prettier"],
            "inputs": ["default", "{workspaceRoot}/.eslintrc.cjs", "{workspaceRoot}/.eslintrc.js"],
            "cache": true
        },
        "lint:prettier": {
            "inputs": ["default", "^public"],
            "cache": true
        },
        "lint:prettier:fix": {
            "inputs": ["default", "^public"],
            "cache": true
        },
        "lint:types": {
            "inputs": ["default", "^public"],
            "cache": true
        },
        "test": {
            "inputs": ["default", "^default", "{projectRoot}/vite.config.ts"],
            "cache": true
        },
        "test:coverage": {
            "inputs": ["default", "^default", "{projectRoot}/vite.config.ts"],
            "cache": true
        },
        "build-storybook": {
            "cache": true
        }
    },
    "parallel": 5,
    "extends": "@nrwl/workspace/presets/npm.json",
    "affected": {
        "defaultBase": "main"
    },
    "defaultBase": "main",
    "defaultProject": "visulima",
    "$schema": "./node_modules/nx/schemas/nx-schema.json"
}
