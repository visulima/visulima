{
    "tasksRunnerOptions": {
        "default": {
            "runner": "nx/tasks-runners/default"
        }
    },
    "targetDefaults": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["default", "^default"],
            "cache": true
        },
        "build:prod": {
            "dependsOn": ["^build:prod"],
            "inputs": ["default", "^default"],
            "cache": true
        },
        "lint:eslint": {
            "dependsOn": ["^build", "^lint:prettier"],
            "inputs": ["default", "{workspaceRoot}/.eslintrc.cjs", "{workspaceRoot}/.eslintrc.js"],
            "cache": true
        },
        "lint:eslint:fix": {
            "dependsOn": ["^build", "^lint:prettier"],
            "inputs": ["default", "{workspaceRoot}/.eslintrc.cjs", "{workspaceRoot}/.eslintrc.js"],
            "cache": true
        },
        "lint:prettier": {
            "inputs": ["default", "^public"],
            "cache": true
        },
        "lint:prettier:fix": {
            "inputs": ["default", "^public"],
            "cache": true
        },
        "lint:types": {
            "inputs": ["^build", "default", "^public"],
            "cache": true
        },
        "test": {
            "inputs": ["default", "^default", "{projectRoot}/vite.config.ts"],
            "cache": true
        },
        "test:coverage": {
            "inputs": ["default", "^default", "{projectRoot}/vite.config.ts"],
            "cache": true
        },
        "build-storybook": {
            "inputs": ["^build", "^build-storybook"],
            "cache": true
        }
    },
    "namedInputs": {
        "default": ["sharedGlobals", "{projectRoot}/**/*", "!{projectRoot}/**/*.md"],
        "public": [
            "default",
            "{workspaceRoot}/dist",
            "!{workspaceRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
            "!{workspaceRoot}/vite.config.ts",
            "!{workspaceRoot}/.eslintrc.json",
            "!{workspaceRoot}/.storybook/**/*",
            "!{workspaceRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
            "!{workspaceRoot}/tsconfig.storybook.json"
        ],
        "sharedGlobals": [
            "{workspaceRoot}/.eslintrc.cjs",
            "{workspaceRoot}/.nvmrc",
            "{workspaceRoot}/package.json",
            "{workspaceRoot}/tsup.config.ts",
            "{workspaceRoot}/tsconfig.json"
        ],
        "production": ["default"]
    },
    "pluginsConfig": {
        "@nrwl/js": {
            "analyzeSourceFiles": false
        }
    },
    "parallel": 5,
    "extends": "@nrwl/workspace/presets/npm.json",
    "affected": {
        "defaultBase": "main"
    },
    "defaultBase": "main",
    "defaultProject": "visulima",
    "$schema": "./node_modules/nx/schemas/nx-schema.json"
}
