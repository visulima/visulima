{
    "name": "docs",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "build:prod": "next build",
        "dev": "next dev",
        "docs:copy": "node scripts/import-package-documentation.js --path ../packages --copy",
        "docs:symlink": "node scripts/import-package-documentation.js --path ../packages --symlink",
        "lint": "eslint . --ext js,cjs,mjs,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs --cache",
        "lint:eslint": "eslint . --ext js,cjs,mjs,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "start": "next start"
    },
    "browserslist": [
        "> 1%",
        "not edge <= 18",
        "not ie 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@code-hike/mdx": "^0.9.0",
        "@next/env": "^14.0.3",
        "@t3-oss/env-nextjs": "^0.7.1",
        "@vercel/og": "^0.5.20",
        "@visulima/nextra-theme-docs": "workspace:*",
        "@visulima/readdir": "workspace:*",
        "clsx": "^2.0.0",
        "fathom-client": "^3.6.0",
        "framer-motion": "^10.16.8",
        "fs-extra": "^11.2.0",
        "lucide-react": "^0.294.0",
        "next": "13.4.19",
        "nextra": "2.13.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "shiki": "^0.14.5",
        "yargs": "^17.7.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^15.0.2",
        "@anolilab/prettier-config": "^5.0.13",
        "@arthurgeron/eslint-plugin-react-usememo": "^2.1.4",
        "@axe-core/react": "^4.8.1",
        "@babel/core": "^7.23.5",
        "@babel/eslint-parser": "^7.23.3",
        "@babel/plugin-syntax-import-assertions": "^7.23.3",
        "@next/eslint-plugin-next": "^14.0.3",
        "@svgr/webpack": "^8.1.0",
        "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
        "@tailwindcss/typography": "^0.5.10",
        "@testing-library/dom": "^9.3.3",
        "@testing-library/react": "^14.1.2",
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/node": "18.18.12",
        "@types/react": "18.2.39",
        "@types/react-dom": "18.2.17",
        "cross-env": "^7.0.3",
        "eslint": "^8.54.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-mdx": "^2.2.0",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-ssr-friendly": "^1.3.0",
        "eslint-plugin-testing-library": "^6.2.0",
        "eslint-plugin-validate-jsx-nesting": "^0.1.1",
        "eslint-plugin-vitest": "^0.3.10",
        "eslint-plugin-vitest-globals": "^1.4.0",
        "eslint-plugin-zod": "^1.4.0",
        "postcss": "^8.4.31",
        "postcss-cli": "^10.1.0",
        "postcss-import": "^15.1.0",
        "postcss-lightningcss": "1.0.0",
        "prettier": "^3.1.0",
        "sort-package-json": "^2.6.0",
        "symlink-dir": "^5.2.0",
        "tailwindcss": "^3.3.5",
        "typescript": "5.3.2",
        "vitest": "^0.34.6"
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false,
            "info_on_disabling_etc_no_deprecated": false,
            "info_on_testing_library_framework": false
        }
    }
}
