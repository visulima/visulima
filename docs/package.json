{
    "name": "docs",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "docs:copy": "node scripts/import-package-documentation.js --path ../packages --copy",
        "docs:symlink": "node scripts/import-package-documentation.js --path ../packages --symlink",
        "lint": "eslint . --ext js,cjs,mjs,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs --cache",
        "lint:eslint": "eslint . --ext js,cjs,mjs,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "start": "next start"
    },
    "browserslist": [
        "> 1%",
        "not edge <= 18",
        "not ie 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@code-hike/mdx": "^0.9.0",
        "@next/env": "^13.4.19",
        "@t3-oss/env-nextjs": "^0.6.1",
        "@vercel/og": "^0.5.11",
        "@visulima/nextra-theme-docs": "workspace:*",
        "@visulima/readdir": "workspace:*",
        "clsx": "^2.0.0",
        "fathom-client": "^3.5.0",
        "framer-motion": "^10.16.1",
        "fs-extra": "^11.1.1",
        "lucide-react": "^0.274.0",
        "next": "13.4.19",
        "nextra": "2.12.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "shiki": "^0.14.3",
        "yargs": "^17.7.2",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^11.3.5",
        "@anolilab/prettier-config": "^5.0.4",
        "@arthurgeron/eslint-plugin-react-usememo": "^1.1.4",
        "@axe-core/react": "^4.7.3",
        "@babel/eslint-parser": "^7.22.11",
        "@babel/plugin-syntax-import-assertions": "^7.22.5",
        "@next/eslint-plugin-next": "^13.4.19",
        "@svgr/webpack": "^8.1.0",
        "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
        "@tailwindcss/typography": "^0.5.9",
        "@testing-library/dom": "^9.3.1",
        "@testing-library/react": "^14.0.0",
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/node": "18.17.12",
        "@types/react": "18.2.21",
        "@types/react-dom": "18.2.7",
        "cross-env": "^7.0.3",
        "eslint": "^8.48.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-mdx": "^2.2.0",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-testing-library": "^6.0.1",
        "eslint-plugin-vitest": "^0.2.8",
        "eslint-plugin-vitest-globals": "^1.4.0",
        "eslint-plugin-zod": "^1.4.0",
        "postcss": "^8.4.29",
        "postcss-cli": "^10.1.0",
        "postcss-import": "^15.1.0",
        "postcss-lightningcss": "0.9.0",
        "prettier": "^3.0.3",
        "sort-package-json": "^2.5.1",
        "symlink-dir": "^5.1.1",
        "tailwindcss": "^3.3.3",
        "typescript": "5.2.2",
        "vitest": "^0.34.3"
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false
        }
    }
}
