        "postcss": "^8.5.6",
        "publint": "^0.3.14",
        "rimraf": "6.0.1",
        "serve": "^14.2.5",
        "sort-package-json": "^3.4.0",
        "storybook": "^9.1.10",
        "tailwindcss": "^4.1.14",
