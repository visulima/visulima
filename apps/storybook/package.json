{
    "name": "@apps/storybook",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "storybook build --docs",
        "clean": "rm -rf .turbo && rm -rf node_modules",
        "dev": "storybook dev -p 6006",
        "lint:eslint": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs",
        "lint:eslint:fix": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs --fix",
        "lint:packagejson": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "preview-storybook": "serve storybook-static"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^15.0.3",
        "@anolilab/prettier-config": "^5.0.14",
        "@arthurgeron/eslint-plugin-react-usememo": "^2.4.0",
        "@babel/core": "^7.24.7",
        "@mdx-js/react": "^3.0.1",
        "@secretlint/secretlint-rule-preset-recommend": "^8.2.4",
        "@storybook/addon-a11y": "^8.1.9",
        "@storybook/addon-actions": "^8.1.9",
        "@storybook/addon-console": "3.0.0",
        "@storybook/addon-docs": "^8.1.9",
        "@storybook/addon-essentials": "^8.1.9",
        "@storybook/addon-interactions": "^8.1.9",
        "@storybook/addon-links": "^8.1.9",
        "@storybook/addon-styling": "^1.3.7",
        "@storybook/addon-themes": "^8.1.9",
        "@storybook/builder-vite": "~8.1.9",
        "@storybook/core-server": "~8.1.9",
        "@storybook/react": "^8.1.9",
        "@storybook/react-vite": "^8.1.9",
        "@storybook/test-runner": "^0.18.2",
        "@storybook/testing-library": "~0.2.2",
        "@testing-library/react": "^16.0.0",
        "@types/node": "18.18.14",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-editorconfig": "^4.0.3",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-mdx": "^3.1.5",
        "eslint-plugin-react": "7.34.2",
        "eslint-plugin-react-hooks": "4.6.2",
        "eslint-plugin-ssr-friendly": "^1.3.0",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-tailwindcss": "^3.17.3",
        "eslint-plugin-testing-library": "^6.2.2",
        "eslint-plugin-validate-jsx-nesting": "^0.1.1",
        "postcss": "^8.4.38",
        "publint": "^0.2.8",
        "serve": "^14.2.3",
        "sort-package-json": "^2.10.0",
        "storybook": "^8.1.9",
        "tailwindcss": "^3.4.4",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.4.5",
        "vite": "^5.3.1",
        "vite-tsconfig-paths": "^4.3.2"
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false,
                "testing-library-dom": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false,
            "info_on_disabling_etc_no_deprecated": false
        }
    }
}
